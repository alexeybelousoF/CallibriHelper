{
  "manifest_version": 2,
  "name": "Callibri Helper",
  "version": "1.0.0",
  "background": {

    "scripts": ["background.js"]

  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"

  },
  "icons":{
    "128":"icon_128.png"
  }
}
